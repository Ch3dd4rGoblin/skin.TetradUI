<?xml version="1.0" encoding="UTF-8"?>
<window>
	<include>dialogeffect</include>
	<coordinates>
		<system>0</system>
		<left>1560</left>
		<top>-4.5</top>
	</coordinates>
	<controls>
		<control type="group">
			<animation effect="slide" start="0,0" end="0,15" time="200" condition="Window.IsVisible(FullscreenVideo) | Window.IsVisible(Visualisation)">conditional</animation>
			<control type="image">
				<description>Media Overlay Image</description>
				<width>300</width>
				<height>127.5</height>
				<bordersize>5,5,5,10</bordersize>
				<texture border="30,30,30,45">dialogs/Dialog2.png</texture>
			</control>
			<control type="grouplist">
				<left>15</left>
				<top>30</top>
				<width>240</width>
				<height>75</height>
				<itemgap>30</itemgap>
				<orientation>horizontal</orientation>
				<align>center</align>
				<control type="image">
					<description>Busy Image</description>
					<width>52.5</width>
					<height>52.5</height>
					<aspectratio>keep</aspectratio>
					<texture>busy.png</texture>
					<animation effect="rotate" start="0" end="360" center="auto" time="1200" loop="true" condition="true">conditional</animation>
				</control>
				<control type="label">
					<width max="180">auto</width>
					<height>52.5</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<label>31302</label>
					<textcolor>orange</textcolor>
				</control>
			</control>
		</control>
	</controls>
</window>
