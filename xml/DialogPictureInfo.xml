<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">5</defaultcontrol>
	<coordinates>
		<system>1</system>
		<left>315</left>
		<top>35</top>
	</coordinates>
	<include>dialogeffect</include>
	<controls>
		<control type="image">
			<description>background image</description>
			<width>650</width>
			<height>650</height>
			<texture border="50,100,50,50">Dialog.png</texture>
			<animation effect="fade" start="90" end="90" time="0" condition="Window.IsVisible(Slideshow)">Conditional</animation>
		</control>
		<control type="label" id="1">
			<description>heading label</description>
			<left>50</left>
			<top>50</top>
			<width>550</width>
			<height>30</height>
			<align>center</align>
			<aligny>center</aligny>
			<font>font16</font>
			<label>13406</label>
			<textcolor>orange</textcolor>
		</control>
		<control type="list" id="5">
			<left>50</left>
			<top>100</top>
			<width>550</width>
			<height>450</height>
			<onup>5</onup>
			<ondown>5</ondown>
			<onleft>5</onleft>
			<onright>60</onright>
			<pagecontrol>60</pagecontrol>
			<scrolltime>200</scrolltime>
			<itemlayout height="45">
				<control type="image">
					<width>550</width>
					<height>45</height>
					<texture border="10">list-nofocus.png</texture>
				</control>
				<control type="label">
					<left>10</left>
					<width>200</width>
					<height>45</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>grey2</textcolor>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<left>215</left>
					<width>325</width>
					<height>45</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<info>ListItem.Label2</info>
				</control>
			</itemlayout>
			<focusedlayout height="45">
				<control type="image">
					<width>550</width>
					<height>45</height>
					<visible>!Control.HasFocus(5)</visible>
					<texture border="10">list-nofocus.png</texture>
				</control>
				<control type="image">
					<width>550</width>
					<height>45</height>
					<visible>Control.HasFocus(5)</visible>
					<texture border="10">list-focus.png</texture>
				</control>
				<control type="label">
					<left>10</left>
					<width>200</width>
					<height>45</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>black</textcolor>
					<info>ListItem.Label</info>
					<visible>Control.HasFocus(5)</visible>
				</control>
				<control type="label">
					<left>10</left>
					<width>200</width>
					<height>45</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>white</textcolor>
					<info>ListItem.Label</info>
					<visible>!Control.HasFocus(5)</visible>
				</control>
				<control type="label">
					<left>215</left>
					<width>325</width>
					<height>45</height>
					<font>font12</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>black</textcolor>
					<info>ListItem.Label2</info>
				</control>
			</focusedlayout>
		</control>
		<control type="spincontrol" id="60">
			<description>Next page Spinner</description>
			<left>540</left>
			<top>567</top>
			<subtype>page</subtype>
			<font />
			<onleft>5</onleft>
			<onright>5</onright>
			<ondown>60</ondown>
			<onup>5</onup>
			<textcolor>white</textcolor>
			<showonepage>true</showonepage>
		</control>
		<control type="label">
			<description>number of files/pages in list text label</description>
			<left>15</left>
			<top>565</top>
			<width>320</width>
			<height>35</height>
			<font>font12</font>
			<align>right</align>
			<aligny>center</aligny>
			<scroll>true</scroll>
			<textcolor>white</textcolor>
			<label>$LOCALIZE[31201] ($INFO[Container(5).CurrentPage]/$INFO[Container(5).NumPages])</label>
		</control>
	</controls>
</window>